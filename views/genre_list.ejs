<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head') -%> 
    <body>
        <div class="container-fluid">
            <div class="row">
                <%- include('partials/sidebar') -%> 
                <div class="col-sm-10">
                    <h1> <%= title %>  </h1>
                    <ul>
                        <% genre_list.forEach(genre => { %>
                             <li>
                                 <a href="<%= genre.url %> ">
                                     <%= genre.name %> 
                                 </a>
                             </li>
                        <% }) %> 
                    </ul>
                </div>
            </div>
        </div>
    </body>
</html>