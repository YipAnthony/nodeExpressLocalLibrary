<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head') -%> 
    <body>
        <div class="container-fluid">
            <div class="row">
                <%- include('partials/sidebar') -%> 
                <div class="col-sm-10">
                   <h1> <%= title %>  </h1>
                   <ul>
                       <% author_list.forEach(author => { %>
                            <li>
                                <a href="<%= author.url %> ">
                                    <%= author.name %> 
                                </a>
                                (<%= author.lifespan %> )
                            </li>
                       <% }) %> 
                   </ul>
                </div>
            </div>
        </div>
    </body>
</html>